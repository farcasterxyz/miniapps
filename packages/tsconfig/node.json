{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "module": "nodeNext",
    "moduleResolution": "nodeNext",
    "target": "es2022",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "rewriteRelativeImportExtensions": true
  }
}

